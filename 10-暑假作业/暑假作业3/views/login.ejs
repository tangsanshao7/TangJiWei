<!DOCTYPE html>
<html>

<head>
  <title>后台管理系统</title>
  <meta charset="UTF-8" />
  <link rel="stylesheet" type="text/css" href="/Css/bootstrap.css" />
  <link rel="stylesheet" type="text/css" href="/Css/bootstrap-responsive.css" />
  <link rel="stylesheet" type="text/css" href="/Css/style.css" />
  <!-- bootstrapvalidator -->
  <link href="https://cdn.bootcdn.net/ajax/libs/bootstrap-validator/0.5.3/css/bootstrapValidator.min.css"
    rel="stylesheet">
  <!-- bootstrapvalidator -->
  <script type="text/javascript" src="/lib/jquery.js"></script>
  <script type="text/javascript" src="/lib/jquery.js"></script>
  <script type="text/javascript" src="/lib/bootstrap.js"></script>
  <script type="text/javascript" src="/lib/ckform.js"></script>
  <script type="text/javascript" src="/lib/common.js"></script>
  <!-- bootstrapvalidator -->
  <script src="https://cdn.bootcdn.net/ajax/libs/bootstrap-validator/0.5.3/js/bootstrapValidator.js"></script>
  <script src="https://cdn.bootcdn.net/ajax/libs/bootstrap-validator/0.5.3/js/language/zh_CN.js"></script>
  <!-- bootstrapvalidator -->


  <style type="text/css">
    body {
      padding-top: 40px;
      padding-bottom: 40px;
      background-color: #f5f5f5;
    }

    .form-signin {
      max-width: 300px;
      padding: 19px 29px 29px;
      margin: 0 auto 20px;
      background-color: #fff;
      border: 1px solid #e5e5e5;
      -webkit-border-radius: 5px;
      -moz-border-radius: 5px;
      border-radius: 5px;
      -webkit-box-shadow: 0 1px 2px rgba(0, 0, 0, 0.05);
      -moz-box-shadow: 0 1px 2px rgba(0, 0, 0, 0.05);
      box-shadow: 0 1px 2px rgba(0, 0, 0, 0.05);
    }

    .form-signin .form-signin-heading,
    .form-signin .checkbox {
      margin-bottom: 10px;
    }

    .form-signin input[type="text"],
    .form-signin input[type="password"] {
      font-size: 16px;
      height: auto;
      margin-bottom: 15px;
      padding: 7px 9px;
    }

    .bv-form .help-block {
      color: red;
    }
  </style>
</head>

<body>
  <div class="container">
    <form class="form-signin form-group" id="form-signin">
      <h2 class="form-signin-heading">登录系统</h2>
      <input type="text" name="username" value="admin" class="input-block-level" placeholder="账号" />
      <input type="password" name="password" value="admin" class="input-block-level" placeholder="密码" />
      <p>
        <button class="btn btn-large btn-primary" type="submit" id="Btnlogin">登录</button>
      </p>
    </form>
  </div>
  <script src="/js/login.js"></script>
</body>

</html>